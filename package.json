{
  "name": "discord-commands",
  "version": "0.0.1",
  "description": "A simple architecture for commands of a discord bot",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "setup": "npm install",
    "build": "tsc",
    "build-watch": "tsc --watch",
    "test-only": "mocha dist/test/",
    "pretest": "npm run build",
    "test": "npm run test-only",
    "clean": "shx rm -rf node_modules/ dist/ docs/",
    "typedoc": "typedoc --out ./docs --mode modules --tsconfig ./tsconfig.json ./src/",
    "posttypedoc": "shx cp .nojekyll docs/.nojekyll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivan94/discord-commands.git"
  },
  "keywords": [
    "discord",
    "bot",
    "commands"
  ],
  "author": "ivan94",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ivan94/discord-commands/issues"
  },
  "homepage": "https://github.com/ivan94/discord-commands#readme",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "shx": "^0.3.2",
    "ts-node": "^7.0.1",
    "typedoc": "^0.13.0",
    "typescript": "^3.1.6"
  }
}
